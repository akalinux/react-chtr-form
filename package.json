{
  "name": "react-chtr-form",
  "version": "1.0.18",
  "description": "Chtr React Data Centric form generation library",
  "main": "src/index.jsx",
  "scripts": {
    "test": "mocha --require babel-register ./test/test.jsx",
    "start": "webpack --watch",
    "build": "webpack",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.corp.chartercom.com:7999/itccc/react-chtr-form.git"
  },
  "keywords": [
    "form",
    "builder",
    "formbuilder",
    "forms",
    "generation"
  ],
  "author": "Michael Shipper <Michael.Shipper@Charter.com>",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "6.x",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "6.x",
    "css-loader": "^0.28.7",
    "jsdom": "^11.3.0",
    "mocha": "4.x",
    "react": "16.x",
    "react-chtr-object-methods": "^1.0.4",
    "react-dom": "^16.0.0",
    "should": "^13.1.2",
    "style-loader": "^0.19.0",
    "unit.js": "2.x",
    "webpack": "^3.8.1",
    "webpack-common-shake": "^1.5.3"
  },
  "babel": {
    "compact": true,
    "presets": [
      [
        "react"
      ],
      [
        "env",
        {}
      ]
    ],
    "plugins": [
      "babel-plugin-transform-react-jsx",
      "transform-class-properties",
      "transform-react-inline-elements"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-react-inline-elements": "^6.22.0"
  }
}
