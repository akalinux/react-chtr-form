{
  "name": "react-chtr-form",
  "version": "1.0.19",
  "description": "Chtr React Data Centric form generation library",
  "main": "src/index.jsx",
  "scripts": {
    "test": "mocha --require babel-register ./test/test.jsx",
    "start": "webpack --watch",
    "build": "babel chtr-form.jsx --out-file src/index.jsx",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.corp.chartercom.com:7999/itccc/react-chtr-form.git"
  },
  "keywords": [
    "form",
    "builder",
    "formbuilder",
    "forms",
    "generation"
  ],
  "author": "Michael Shipper <Michael.Shipper@Charter.com>",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "1.x",
    "babel-preset-react": "6.x",
    "babel-register": "6.x",
    "css-loader": "^0.x",
    "mocha": "4.x",
    "react": "16.x",
    "react-chtr-object-methods": "^1.x",
    "react-dom": "^16.x",
    "unit.js": "2.x"
  },
  "babel": {
    "compact": true,
    "presets": [
      [
        "react"
      ],
      [
        "env",
        {}
      ]
    ],
    "plugins": [
      "babel-plugin-transform-react-jsx",
      "transform-class-properties",
      "transform-react-inline-elements"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0"
  }
}
